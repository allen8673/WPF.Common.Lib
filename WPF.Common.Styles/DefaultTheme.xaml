<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WPF.Common.Styles">
    <SolidColorBrush x:Key="NormalForegroundBrush" Color="#464646" />
    <SolidColorBrush x:Key="NormalBorderBrush" Color="#C8C8C8" />
    <SolidColorBrush x:Key="NormalBackgroundBrush" Color="#fff" />
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="#888" />
    <SolidColorBrush x:Key="DisabledBackgroundBrush" Color="#EBEAEA" />
    <SolidColorBrush x:Key="DisabledBorderBrush" Color="#C8C8C8" />
</ResourceDictionary>